<t tx="vitalije.20190708082017.1">#[test]
fn b64_works() {
  for i in 0..1000 {
    let s = b64str(i);
    let s1 = b64str(i + 0x1_00_0000);
    assert_eq!(i, b64int(&amp;s));
    assert_eq!(i, b64int(&amp;s1) - 0x1_00_0000);
  }
}
