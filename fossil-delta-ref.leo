        'mode/coffeescript/coffeescript.js': 'codemirror-mode-coffeescript',
        'addon/mode/simple.js': 'codemirror-mode-simple'
    }
    ew = tuple(cmfiles.keys())
    for n in nlist:
        if n.endswith(ew):
            for k in ew:
                if n.endswith(k):
                    v = cmfiles[k]
                    c.db[v] = zf.read(n).decode('utf8')
